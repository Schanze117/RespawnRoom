    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.503.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pubnub": "^7.6.2",
    "react-loading-skeleton": "^3.5.0",
    "react-youtube": "^10.1.0"
  },
  "devDependencies": {
    "@types-date-fns": "^2.5.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "wait-on": "^7.2.0"
  }
